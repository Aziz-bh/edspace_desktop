<AnchorPane id="AnchorPane" prefHeight="464.0" prefWidth="711.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.edspace.gui.LoginController">
   <children>
      <ImageView fx:id="logo" fitHeight="157.0" fitWidth="511.0" layoutX="50.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../images/logo2.png" />
         </image>
      </ImageView>
      <Pane layoutX="122.0" layoutY="200.0" prefHeight="222.0" prefWidth="434.0">
         <children>
            <TextField fx:id="emailLogin" layoutX="114.0" layoutY="23.0" prefHeight="25.0" prefWidth="221.0" />
            <TextField fx:id="pwdLogin" layoutX="114.0" layoutY="98.0" prefHeight="25.0" prefWidth="221.0" />
            <Label layoutX="114.0" layoutY="81.0" text="Mot de pasee" />
            <Label layoutX="114.0" layoutY="6.0" text="Email" />
            <Button fx:id="btnLogin" layoutX="194.0" layoutY="183.0" mnemonicParsing="false" onAction="#login" text="Login" />
            <Button fx:id="mdpOublie" layoutX="150.0" layoutY="150.0" mnemonicParsing="false" onAction="#mdp" style="-fx-background-color: #FFFFFF;" text="Mot de passe oublier !" textFill="#61dfc1" />
            <Text fx:id="nullErr" disable="true" fill="RED" layoutX="153.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="veuillez entrer votre email !" visible="false" />
            <Text fx:id="nullmdp" fill="RED" layoutX="132.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="veuillez entrer votre mot de passe !" visible="false" />
         </children>
      </Pane>
   </children>
</AnchorPane>
