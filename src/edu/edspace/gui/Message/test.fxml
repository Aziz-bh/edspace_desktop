<AnchorPane id="AnchorPane" prefHeight="519.0" prefWidth="1066.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.edspace.gui.Message.TestController">
