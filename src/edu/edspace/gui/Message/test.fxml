            <Button fx:id="send" layoutX="171.0" layoutY="298.0" mnemonicParsing="false" onAction="#sendMessage" prefHeight="37.0" prefWidth="49.0" style="-fx-background-color: #54b48c;" text="Send" textFill="WHITE" />
